<h3>User.Identities</h3>
@if (Model?.Identities != null)
{
    foreach (var identity in @Model?.Identities)
    {
        <table class="form">
            <tr>
                <td>Name</td>
                <td>@identity.Name</td>
            </tr>
            <tr>
                <td class="pl-4">AuthenticationType</td>
                <td>@identity.AuthenticationType</td>
            </tr>
            <tr>
                <td class="pl-4">IsAuthenticated</td>
                <td>@identity.IsAuthenticated</td>
            </tr>
            <tr>
                <td class="pl-4">Label</td>
                <td>@identity.Label</td>
            </tr>
            <tr>
                <td class="pl-4">Actor</td>
                <td>@identity.Actor</td>
            </tr>
            <tr>
                <td class="pl-4">NameClaimType</td>
                <td>@identity.NameClaimType</td>
            </tr>
            <tr>
                <td class="pl-4">RoleClaimType</td>
                <td>@identity.RoleClaimType</td>
            </tr>
            <tr>
                <td class="pl-4">Claims</td>
                <td>
                    <table>
                        <thead>
                            <tr>
                                <th class="small">Subject</th>
                                <th class="small">Type</th>
                                <th class="small">Value</th>
                                <th class="small">ValueType</th>
                                <th class="small">Issuer</th>
                                <th class="small">OriginalIssuer</th>
                                <th class="small">Properties</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (identity.Claims != null)
                            {
                                foreach (var claim in identity.Claims)
                                {
                                    <tr>
                                        <td>@claim.Subject</td>
                                        <td>@claim.Type</td>
                                        <td>@claim.Value</td>
                                        <td>@claim.ValueType</td>
                                        <td>@claim.Issuer</td>
                                        <td>@claim.OriginalIssuer</td>
                                        <td>
                                            @foreach (var prop in claim.Properties)
                                            {
                                                @prop.Key @prop.Value<br />
                                            }
                                        </td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </td>
            </tr>
        </table>
    }
}

